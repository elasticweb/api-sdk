{
  "name": "elasticweb/api-sdk",
  "description": "API SDK for elasticweb.org",
  "keywords": ["api", "hosting"],
  "type": "library",
  "require": {
    "php": ">=5.5.0",
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/support": "5.0.*|5.1.*|5.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.0",
    "phpspec/prophecy": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Elasticweb\\Api\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Elasticweb\\Api\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
